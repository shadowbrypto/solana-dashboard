    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "papaparse": "^5.4.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@types/papaparse": "^5.3.14",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3"
