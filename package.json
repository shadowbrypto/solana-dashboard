{
  "name": "dashboard",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "start": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "db:update_csv": "sh scripts/update.sh",
    "db:migrate": "npx tsx scripts/importCsvToSupabase.ts",
    "db:force_migrate": "npx tsx scripts/importCsvToSupabaseWithDelete.ts",
    "server:install": "cd server && npm install",
    "server:dev": "cd server && npm run dev",
    "server:build": "cd server && npm run build",
    "server:start": "cd server && npm start",
    "dev:full": "concurrently \"npm run server:dev\" \"npm run dev\"",
    "start:full": "concurrently \"npm run server:start\" \"npm run start\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.2.11",
