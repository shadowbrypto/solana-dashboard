{
  "name": "sol-analytics-backend",
  "version": "1.0.0",
  "description": "Backend API for Sol Analytics Dashboard",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "tsx src/index.ts",
    "migrate-db": "tsx scripts/run-migrations.ts",
    "fetch-all-trader-stats": "tsx -e \"import('./src/services/duneTraderStatsService.js').then(m => m.DuneTraderStatsService.fetchAllProtocolTraderStats())\""
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.49.4",
    "cors": "^2.8.5",
    "csv-parse": "^6.1.0",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "mysql2": "^3.15.3",
    "papaparse": "^5.4.1",
    "tinyglobby": "^0.2.15",
    "uuid": "^13.0.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@types/papaparse": "^5.3.14",
    "@types/uuid": "^10.0.0",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3"
  }
}
